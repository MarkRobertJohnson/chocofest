##AH:UseTextMode
module BootStrapFeeds
{
    PSDsc chocolatey::ChocolateySource
    (
        Otter_ConfigurationKey: Name,
        Name: Default,
        Ensure: Present,
        Source: $ChocolateyFeedUrl
    );

    # Try/Catch
    try
    {
        PSDsc chocolatey::ChocolateySource
        (
            Otter_ConfigurationKey: Name,
            Name: chocolatey,
            Ensure: Absent,
            Source: https://chocolatey.org/api/v2/
        );
    }
    catch
    {
        Log-Warning Failed to remove chocolatey default source;
    }
}
